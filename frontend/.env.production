# Frontend Environment Variables for Production

# Disable ESLint warnings during build to prevent CI failures
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=true

# API Configuration
# Update this URL after deploying your backend to Vercel
REACT_APP_BASE_URL=https://your-backend-app.vercel.app

# Build Configuration
GENERATE_SOURCEMAP=false